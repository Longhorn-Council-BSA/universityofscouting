        <form action="/login" method="post">
          <div class="form-group row">
            <div class="col">
              <input type="text" class="form-control" id="inputPassword1" name="password" placeholder="Last Name">
            </div>
          </div>
          <div class="form-group row">
            <div class="col">
              <input type="password" class="form-control" id="inputUsername1" name="username" placeholder="Member ID">
            </div>
          </div>
          <div>
              <input type="submit" value="Log In" class="btn btn-sm nav-btn-1"/>
          </div>
        </form>
      </div>
    </div>
  </div>
<%- include footer %>
